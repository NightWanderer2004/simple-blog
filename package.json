{
  "name": "simple-blog",
  "scripts": {
    "start": "eleventy --serve & npm run tailwind",
    "build": "rm -rf dist && eleventy && npx tailwindcss -i ./src/styles/index.css -o ./src/styles/output.css",
    "tailwind": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/output.css --watch --minify"
  },
  "author": "NightWandeder2004",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "tailwindcss": "^3.0.24"
  }
}
